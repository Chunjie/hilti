#
# @TEST-EXEC:  printf "   1 23   456"  | pac-driver-test -p Test::test_pre  %INPUT >>output
# @TEST-EXEC:  printf "123456"         | pac-driver-test -p Test::test_pre  %INPUT >>output
# @TEST-EXEC:  printf "1    23 456"    | pac-driver-test -p Test::test_post %INPUT >>output
# @TEST-EXEC:  printf "123456"         | pac-driver-test -p Test::test_post %INPUT >>output
# @TEST-EXEC:  printf "AAA1BBBA23B456" | pac-driver-test -p Test::test_both %INPUT >>output
# @TEST-EXEC:  printf "123456"         | pac-driver-test -p Test::test_both %INPUT >>output
# @TEST-EXEC:  btest-diff output
#

module Test;

export type test_pre = unit {
  %skip-pre = / */;

  a: bytes &length=1;
  b: bytes &length=2;
  c: bytes &length=3;

  on %done {
    print self;
    }
};

export type test_post = unit {
  %skip-pre = / */;

  a: bytes &length=1;
  b: bytes &length=2;
  c: bytes &length=3;

  on %done {
    print self;
    }
};

export type test_both = unit {
  %skip-pre = /A*/;
  %skip-post = /B*/;

  a: bytes &length=1;
  b: bytes &length=2;
  c: bytes &length=3;

  on %done {
    print self;
    }
};
