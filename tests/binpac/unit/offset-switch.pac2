# @TEST-EXEC:  btest-diff output

module Mini;

type TestEnum = enum {
  me = 0
};

export type Test = unit {
  a: uint16 &convert=TestEnum($$) {
    print (10 - self.offset());
  }

  switch ( self. a ) {
    * -> d: bytes &length=(10 - self.offset()) {
      print |self.d|;
    }
  };
};
